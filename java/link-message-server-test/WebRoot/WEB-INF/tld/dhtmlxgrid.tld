			<rtexprvalue>true</rtexprvalue>
			<description>列标题</description>
		</attribute>
		
		<attribute>
			<name>width</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>宽度</description>
		</attribute>
		
		<attribute>
