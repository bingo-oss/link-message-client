	<tag>
		<name>column</name>
		<tag-class>
			bingo.ui.grid.tag.ColumnTag
		</tag-class>
		<body-content>JSP</body-content>
		<description>列表显示列Tag配置</description>
		
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<name>header</name>
